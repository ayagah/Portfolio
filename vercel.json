{
  "version": 2,
  "builds": [
    {
      "src": "Portflio_Project/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "pythonVersion": "3.9"
      }
    },
    {
      "src": "staticfiles/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/staticfiles/$1"
    },
    {
      "src": "/(.*)",
      "dest": "Portflio_Project/wsgi.py"
    }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "Portflio_Project.settings",
    "PYTHONPATH": "/var/task",
    "SECRET_KEY": "@django-secret-key",
    "DEBUG": "False",
    "ALLOWED_HOSTS": ".vercel.app,.now.sh"
  }
}